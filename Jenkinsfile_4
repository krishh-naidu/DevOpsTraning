pipeline {
    agent none
    stages{
        stage ('Build'){
            steps{
                echo "this is build stage"
            }
        }
        stage ('Deploy'){
          steps{
          input (
                message: "Ready to continue?",
                ok: "Yes",
                submitter: "ssbostan,admin,admins,managers",
                submitterParameter: "SUBMITTER_USERNAME",
            )
              echo "this is deployment stage"
          }
      }
    }
}
