pipeline {
    agent none
    stages{
        stage ('Build'){
            steps{
                echo "this is build stage"
            }
        }
        stage ('Deploy'){
          steps{
          input(
                    message: "Ready to continue?",
                    ok: "Yes",
                    submitter: "ssbostan"
                    parameters {
                    string(name: "DEPLOY_ENV", defaultValue: "production")
                }
                )
              echo "this is deployment stage"
          }
      }
    }
}
