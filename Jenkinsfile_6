pipeline {
  agent any

  stages {
   stage ("Clone GIT Repo"){
     steps {
      dir("Jenkins_Training") {
       git(
         url: "https://github.com/krishh-naidu/DevOpsTraning.git",
         branch: "Jenkins_Training",
         changelog: true,
         poll: true
       )
      }
     }
   } 
  }
}
